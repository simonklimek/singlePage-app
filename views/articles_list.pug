extends layout

    block content
    div.row 
        h1 SEARCH 
        div.col.s12 
            div.card-panel.blue-grey.lighten-5 
                div.row.valign-wrapper 
                    div.input-field.col.s9 
                        i.material-icons.prefix subdirectory_arrow_right 
                        input.autocomlete(type="text", id="autocomplete-input")
                        label(for="autocomplete-input") Autocomplete article search 
                    div.col.s3 
                        button.btn.waves-effect.orange.waves-light(type="submit", name="action") Search 
                            i.material-icons.right search 
